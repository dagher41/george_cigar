    "babel-preset-stage-0": "^6.24.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.3.1",
    "compression": "^1.7.3",
    "connect-ensure-login": "^0.1.1",
    "connect-flash": "^0.1.1",
    "connect-session-sequelize": "^6.1.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
