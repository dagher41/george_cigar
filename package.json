{
  "name": "webpack_practice",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "console": "CONSOLE_ACCESS=true babel-node --presets @babel/preset-env",
    "bs": "npm run clean && npm run prod:client && npm run prod:server && npm run prod:start",
    "db:seed": "sequelize db:seed:all",
    "db:setup": "NODE_ENV=development sequelize db:create && sequelize db:migrate && npm run db:seed",
    "db:reset": "NODE_ENV=development sequelize db:drop && npm run db:setup",
    "db:migrate": "sequelize db:migrate",
    "db:data": "MIGRATION_TYPE=data_migration sequelize db:migrate",
    "db:restore": "sequelize db:drop && sequelize db:create && pg_restore --verbose --clean --no-acl --no-owner -h localhost -U root -d george_cigar_development database/latest.dump",
    "db:data:undo": "MIGRATION_TYPE=data_migration sequelize db:migrate:undo",
    "db:dummy": "MIGRATION_TYPE=dummy_data sequelize db:migrate",
    "db:dummy:undo": "MIGRATION_TYPE=dummy_data sequelize db:migrate:undo",
    "prod:client": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "prod:server": "cross-env NODE_ENV=production webpack --config webpack.config.server.js",
    "prod:start": "cross-env NODE_ENV=production cross-env NODE_ENV=production node index.js",
    "start": "nodemon index.js"
  },
  "author": "",
  "license": "ISC",
